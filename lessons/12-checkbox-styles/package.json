{
  "name": "12-checkbox-styles",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch src/sass:src/css",
    "watch:css": "postcss src/css/*.css --watch -u autoprefixer --dir public/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass --no-source-map src/sass:src/css",
    "build:eleventy": "eleventy",
    "postbuild": "postcss public/css/*.css -r -u autoprefixer cssnano",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "author": "5t3ph",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "autoprefixer": "^10.0.4",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.1.0",
    "postcss": "^8.1.0",
    "sass": "^1.26.10"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
